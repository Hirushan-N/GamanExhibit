{
    "endpoints": [
      {
        "method": "POST",
        "path": "/tickets",
        "summary": "Create a new ticket",
        "tags": ["Tickets"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": [
                  {
                    "name": "commuterPhone",
                    "type": "string",
                    "description": "Commuter's phone number",
                    "example": "1234567890"
                  },
                  {
                    "name": "tripId",
                    "type": "string",
                    "description": "The ID of the trip",
                    "example": "1"
                  },
                  {
                    "name": "busId",
                    "type": "string",
                    "description": "The ID of the bus",
                    "example": "12"
                  }
                ]
              }
            }
          }
        },
        "responses": [
          { "status": 201, "description": "Ticket created successfully" },
          { "status": 400, "description": "Bad request" }
        ]
      }
    ]
  }
  